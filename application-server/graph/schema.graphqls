type Vulnerability {
  id: ID!
  description: String!
  dependency: Dependency!
}

type Dependency {
  id: ID!
  name: String!
  version: String!
}

type Image {
  id: ID!
  repository: String!
  tag: String!
  dependencies: [Dependency!]!
}

type Device {
  id: ID!
  name: String!
  image: Image!
}

type Query {
  vulnerabilities: [Vulnerability!]!
  devices: [Device!]!
  images: [Image!]!
}
